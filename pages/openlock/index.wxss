/* pages/openlock/index.wxss */

.open-list {
  height: 250rpx;
  margin-bottom: 20rpx;
}

.top {
  height: 70rpx;
  border-bottom: 1rpx solid #222;
  padding: 0 20rpx;
  line-height: 70rpx;
  font-size: 27rpx;
  color: #5a5a5a;
}

.down {
  height: 180rpx;
  padding: 20rpx;
}

.down-l {
  width: 50%;
  font-size: 27rpx;
  color: #5a5a5a;
}

.down-l .id text:nth-of-type(1) {
  font-size: 37rpx;
}
.down-l .id text:nth-of-type(2) {
  padding:0 10rpx;
  height: 38rpx;
  display: inline-block;
  line-height: 38rpx;
  border-radius: 5rpx;
  margin-left: 30rpx;
}
.down-r image{
  width: 25rpx;
  height: 30rpx;
  margin-top: 60rpx;
}
.down navigator{
  width: 100%;
  height: 100%;
  display: block;
}
