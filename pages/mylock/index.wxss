/* pages/mylock/index.wxss */
page{
  padding: 20rpx;
  padding-bottom: 115rpx;
}
.main-id,.main-btn{
  font-size: 27rpx;
  height: 105rpx;
  line-height: 105rpx;
  background-color: #fff;
  border-bottom: 1px solid #e1e1e1;
  padding: 0 20rpx;
  color: #333;
}
.main-status{
  padding: 0 10rpx;
  height: 38rpx;
  margin-top: 30rpx;
  line-height: 38rpx;
  border-radius: 5rpx;
  background-color: #b3a06a;
}
.main-btn{
  border: none;
}
.main-btn view{
  width: 150rpx;
  height: 60rpx;
  border-radius: 60rpx;
  border: 1px solid #b3a06a;
  margin-top: 23rpx;
  line-height: 60rpx;
  text-align: center;
  color: #b3a06a;
}
.main-btn image{
  width:30rpx;
  height:30rpx;
  float: left;
  margin-top: 15rpx;
  margin-left: 20rpx;
}
.my-second{
  background-color: #2b2b2b;
}
.second-id{
  font-size: 27rpx;
  height: 105rpx;
  line-height: 105rpx;
  background-color: #fff;
  border-bottom: 1px solid #e1e1e1;
  padding: 0 20rpx; 
  color: #333;
}
.second-id:last-of-type{
  border: none;
}
.mylock-sever{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 105rpx;
  line-height: 105rpx;
  text-align: center;
  background-color: #fff;
  color: #b3a06a;
}
.mylock-sever>view{
  width: 50%;
  height: 100%;
  padding: 0rpx 80rpx;
}
.mylock-sever>view:nth-of-type(1){
  border-right: 1rpx solid #b3a06a;
}
.mylock-sever image{
  width: 42rpx;
  float: left;
  margin-top: 32rpx;
}