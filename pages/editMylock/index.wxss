/* pages/editMylock/index.wxss */
page{
  height: 100%;
}
.list {
  height: 105rpx;
  line-height: 105rpx;
  background-color: #2b2b2b;
  font-size: 27rpx;
  padding: 0 20rpx;
  border-bottom: 1px solid #222;
}
.list picker{
  color: #29a1f7;
}
.list-btn{
  position: absolute;
  bottom: 100rpx;
  left: 0;
  width: 100%;
  height: 100rpx;
  z-index: 99;
}
.list-btn button{
  width: 650rpx;
  margin: 0 auto;
  height: 92rpx;
  line-height: 92rpx;
  border-radius: 92rpx;
  font-size: 31rpx;
}
