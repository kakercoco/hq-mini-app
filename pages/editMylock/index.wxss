/* pages/editMylock/index.wxss */
page{
  height: 100%;
  background-color: #efeeeb;
}
.list {
  height: 105rpx;
  line-height: 105rpx;
  background-color: #fff;
  font-size: 27rpx;
  padding: 0 20rpx;
  border-bottom: 1px solid #f3f3f3;
  color: #333;
}
.list picker{
  color: #333;
}
.list image{
  width: 34rpx;
  margin-top: 35rpx;
  margin-right: 35rpx;
}
.list-btn{
  position: absolute;
  bottom: 100rpx;
  left: 0;
  width: 100%;
  height: 100rpx;
  z-index: 99;
}
.list-btn button{
  width: 650rpx;
  margin: 0 auto;
  height: 92rpx;
  line-height: 92rpx;
  border-radius: 92rpx;
  font-size: 31rpx;
  color: #fff;
}
